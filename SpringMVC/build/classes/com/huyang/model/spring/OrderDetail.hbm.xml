<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.huyang.model.spring.OrderDetail" table="orderDetail">
        <id name="id" column="id" type="java.lang.Integer">
            <generator class="native"/>
        </id>
         <property name="status" column="status" type="java.lang.String"/>
       <property name="orderID" column="orderID" type="java.lang.String"/>
        <property name="currentPrice" column="currentPrice" type="double"/>
        <property name="productID" column="productID" type="java.lang.Integer"/>
        <property name="count" column="count" type="java.lang.Integer"/>
        <property name="addressID" column="addressID" type="java.lang.Integer"/>
       <property name="createTime" column="createTime" type="timestamp"/>
    </class>
</hibernate-mapping>